{
  "name": "my-qwik-basic-starter",
  "description": "Demo App with Routing built-in (recommended)",
  "engines": {
    "node": ">=16.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build-storybook": "storybook build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/vercel-edge/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "cypress.open": "cypress open --component",
    "cypress.run": "cypress run --component",
    "deploy": "vercel deploy",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "storybook": "storybook dev -p 6006",
    "test.e2e": "playwright test",
    "test.unit": "vitest components",
    "test.unit.ui": "vitest --ui components",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/partytown": "0.8.0",
    "@builder.io/qwik": "1.1.5",
    "@builder.io/qwik-city": "1.1.5",
    "@playwright/test": "1.35.1",
    "@storybook/addon-essentials": "7.0.23",
    "@storybook/addon-links": "7.0.23",
    "@storybook/blocks": "7.0.23",
    "@storybook/builder-vite": "7.0.23",
    "@storybook/html": "7.0.23",
    "@storybook/html-vite": "7.0.23",
    "@types/eslint": "8.40.2",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "@vitest/ui": "0.32.2",
    "autoprefixer": "10.4.14",
    "cypress": "12.15.0",
    "cypress-ct-qwik": "0.3.0",
    "eslint": "8.43.0",
    "eslint-plugin-qwik": "1.1.5",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.3.0",
    "storybook": "7.0.23",
    "storybook-framework-qwik": "0.2.2",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3",
    "undici": "5.22.1",
    "vercel": "30.2.3",
    "vite": "4.3.9",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.32.2"
  }
}
